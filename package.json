{
  "name": "oddin-documentation",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:proj": "docusaurus start --config docusaurus.config.second.ts",
    "build": "docusaurus build",
    "build:proj": "docusaurus build --config docusaurus.config.second.ts --out-dir build/projects",
    "build:all": "npm run build && npm run build:proj -- --out-dir build/projects",
    "swizzle": "docusaurus swizzle",
    "swizzle:proj": "docusaurus swizzle --config docusaurus.config.second.ts",
    "deploy": "docusaurus deploy",
    "deploy:proj": "docusaurus deploy --config docusaurus.config.second.ts",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "serve:proj": "docusaurus serve --config docusaurus.config.second.ts",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@types/react": "^18.2.29",
    "typescript": "~5.5.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
